import java.util.*;

public class Solution {

	public static void main(String[] args) {
		Scanner sca = new Scanner(System.in);
		System.out.println("================================");
		for (int i = 0; i < 3; i++) {
			String cadena = sca.next();
			int x = sca.nextInt();
			System.out.printf("%s %d", cadena , x);
		}
	}

}
